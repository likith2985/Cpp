#include <iostream>
using namespace std;
int max(int a, int b) {
    return (a > b) ? a : b;
}
double max(double a, double b) {
    return (a > b) ? a : b;
}
char max(char a, char b) {
    return (a > b) ? a : b;
}

int main() {
    cout << "Maximum of 10 and 20: " << max(10, 20) << endl;
    cout << "Maximum of 3.14 and 2.71: " << max(3.14, 2.71) << endl;
    cout << "Maximum of 'A' and 'Z': " << max('A', 'Z') << endl;
    return 0;
}
